//wrong answer
class Solution {
    public boolean canWinNim(int n) {
        if(n < 4) return true;
        if(n = 4) return false;
        if((n - 2) % 3 % 2) true;
    }
}

//right answer
class Solution {
    public boolean canWinNim(int n) {
        if(n%4 == 0) return false;
        else return true;
    }
}
